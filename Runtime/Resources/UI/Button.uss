.unity-button:hover:enabled {
    background-image: url("project://database/Assets/Menu/UIKit/Images/ButtonPrimary.png?fileID=21300000&guid=fb5edd020a068434bb4e116817931b5b&type=3#ButtonPrimary");
    transition-duration: 100ms;
}

.unity-button-highlighted {
    background-image: url("project://database/Assets/Menu/UIKit/Images/ButtonPrimary.png?fileID=21300000&guid=fb5edd020a068434bb4e116817931b5b&type=3#ButtonPrimary");
    -unity-background-scale-mode: stretch-to-fill;
}